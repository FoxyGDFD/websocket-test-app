{
  "name": "vite-react-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "lint:fix": "eslint . --report-unused-disable-directives --fix",
    "format:fix": "npx prettier --write .",
    "lint": "npx eslint . --report-unused-disable-directives",
    "format": "npx prettier --check .",
    "preview": "vite preview",
    "test": "vitest",
    "prepare": "husky",
    "test:unit": "vitest run --coverage",
    "generate-routes": "npx tsr generate",
    "watch-routes": "npx tsr watch"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.14",
    "@tanstack/react-query": "^5.77.2",
    "@tanstack/react-router": "^1.120.11",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/ui": "^3.1.4",
    "axios": "^1.9.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "^4.1.14",
    "valtio": "^2.1.8",
    "vitest": "^3.1.4",
    "zod": "^3.25.32"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@tanstack/react-query-devtools": "^5.77.2",
    "@tanstack/react-router-devtools": "^1.120.11",
    "@tanstack/router-plugin": "^1.120.11",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^22.15.23",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-sonarjs": "^3.0.2",
    "eslint-plugin-unicorn": "^59.0.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.33.0",
    "vite": "^6.3.5"
  }
